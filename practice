#include <iostream>
using namespace std;

int fibonacci(int n) {
  if (n < 2)
    return 1;
  return fibonacci(n-1) + fibonacci (n-2);
}


int main() {
  //int n;
  //cout << "Enter n: ";
  //cin >> n;
  //cout << "Position " << n << " = " << fibonacci(n) << endl;
  for (int i = 0; i < 20; i++)
    cout << fibonacci(i) << endl;
}
